
material Bullets/Bullet1
{
	technique
	{
		pass
		{
			lighting off
			scene_blend alpha_blend
			depth_check on
			depth_write off
			point_sprites on
			//point_size 10.0
			//point_size_min 0.0 
			//point_size_max 10.0
